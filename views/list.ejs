<%- include("header") -%> 

        <div class="box" id="heading">
            <h1><%= listTitle %></h1>
        </div>


            <div class="box">
                <% listOfItem.forEach(function(item){ %>
                <div class="item">
                    <input type="checkbox">
                    <p><%= item %></p>
                </div>          
                <% });%>

                <form class="item" action="/" method="POST">
                    <input type="text" name="listItem" placeholder="New Item" autocomplete="off">
                     <button type="submit" name="button" value = <%= listTitle %>>+</button>
                </form>
            </div>

 <%- include("footer") -%> 
